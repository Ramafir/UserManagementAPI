{
    "name": "usermanagement-api",
    "version": "1.0.0",
    "description": "User Management API | Recruitment Exlabs Task",
    "main": "src/index.ts",
    "scripts": {
        "dc-up": "docker-compose --env-file .env -f docker-compose.yml up -d",
        "dc-down": "docker-compose --env-file .env -f docker-compose.yml down",
        "dc-test-up": "docker-compose --env-file .env.test -f docker-compose.yml up -d",
        "dc-test-down": "docker-compose --env-file .env.test -f docker-compose.yml down",
        "db-migrate": "./node_modules/.bin/sequelize-cli db:migrate",
        "db-seed": "./node_modules/.bin/sequelize-cli db:seed:all",
        "db-reset": "npm run db-drop && npm run db-create && npm run db-migrate && npm run db-seed",
        "db-drop": "./node_modules/.bin/sequelize-cli db:drop",
        "db-create": "./node_modules/.bin/sequelize-cli db:create",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "NODE_ENV=test jest --detectOpenHandles --forceExit"
    },
    "engines": {
        "node": "^20.0.0",
        "npm": "^10.0.0"
    },
    "_moduleAliases": {
        "@": "./src"
    },
    "author": "daniel.kolban",
    "license": "ISC",
    "dependencies": {
        "cors": "2.8.5",
        "dotenv": "16.4.7",
        "express": "4.21.2",
        "express-async-errors": "3.1.1",
        "helmet": "8.0.0",
        "sequelize": "6.37.5",
        "sequelize-typescript": "2.1.6"
    },
    "devDependencies": {
        "@types/cors": "2.8.17",
        "@types/express": "5.0.0",
        "prettier": "3.4.2",
        "ts-node": "10.9.2",
        "ts-node-dev": "2.0.0",
        "typescript": "5.7.3"
    }
}
